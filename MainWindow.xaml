<Window x:Class="Automatische_Bewegung.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Automatische_Bewegung"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas>
            <Rectangle x:Name="box" Fill="Green" Height="10" Width="10" Canvas.Bottom="100" Canvas.Left="0"/>
            <Button x:Name="btn_start" Content="Start" Canvas.Top="0" Canvas.Left="0" Click="btn_start_Click"/>
        </Canvas>
    </Grid>
</Window>
